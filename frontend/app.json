{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain", 
      "backgroundColor": "#E4E0E1"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UiSupportsMultipleWindows": true
      }
    },
    "android": {
      
      
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.frontend",
      "orientation": "portrait",
      "navigationBar": {
        "backgroundColor": "#E4E0E1"
      },
      "statusBar": {
        "backgroundColor": "#E4E0E1",
        "barStyle": "dark-content"
      }
      
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#E4E0E1"
        }
      ],
      "expo-font",
      [
        "expo-system-ui",
        {
          "userInterfaceStyle": "automatic"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}